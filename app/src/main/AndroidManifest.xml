<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sxnwlfkk.dailyroutines">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_watch_black_24dp"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".views.mainActivity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".views.editActivity.EditActivity"
            android:theme="@style/AppTheme"
            android:parentActivityName="com.sxnwlfkk.dailyroutines.views.mainActivity.MainActivity" >
        </activity>
        <activity
            android:name=".views.routineActivity.RoutineActivity"
            android:theme="@style/AppTheme"
            android:parentActivityName="com.sxnwlfkk.dailyroutines.views.mainActivity.MainActivity" >
        </activity>
        <provider
            android:name=".data.RoutineProvider"
            android:authorities="com.sxnwlfkk.dailyroutines"
            android:exported="false" />
    </application>
</manifest>